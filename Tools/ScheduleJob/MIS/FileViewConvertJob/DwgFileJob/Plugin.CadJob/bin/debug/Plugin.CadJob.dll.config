<?xml version="1.0"?>
<configuration>
  <connectionStrings>
		<add name="FileStore" connectionString="data source=.;Initial Catalog=DFDL_FileStore;User ID=sa;Password=admin123" providerName="System.Data.SqlClient" />
	</connectionStrings>
  <appSettings>
    <!---打印配置-->
    <add key="Printer" value="DWG To PDF.pc3"/>
    <add key="CanonicalMediaName" value="用户 1 (4000.00 x 3000.00 毫米)"/>
    <add key="PlotStyleSheet" value="acad.ctb"/>

    <!--FileStore服务器-->
    <add key="FileStore" value="http://localhost:8060/FileStore/Services/OuterService.asmx"/>

    <!--转换后保存的路径 D:\FileStoreFolder\Convert-->
    <add key="CacheViewFilePath" value="E:\Tools\LeafletViewSite\LeafletViewSite\Files" />

    <!--转换类型:SWFViewer(SWF游览) PDFViewer(PDF游览) TilePicViewer(TilePic浏览)-->
    <add key="ViewMode" value="PDFViewer" />
  </appSettings>
  <startup><supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5"/></startup>
  <system.serviceModel>
    <bindings/>
    <client/>
  </system.serviceModel>
</configuration>
