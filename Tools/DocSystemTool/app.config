<?xml version="1.0"?>
<configuration>
  <appSettings>
    <!--本站点使用的文件服务器名-->
    <add key="FS_ServerName" value="Base" />
    <!--主文件服务器站点的Url-->
    <add key="FS_MasterServerUrl" value="http://localhost/FileStore/Services/MasterService.asmx" />
    <!--是否分布式存储,一般情况请配置为True-->
    <add key="FS_Distributed" value="True" />
    <!--是否主站点-->
    <add key="FS_IsMaster" value="True" />

    <!--是否启用分布式事务-->
    <add key="UseMsdtc" value="False" />
    <!-- 是否启用缓存-->
    <add key="UseCache" value="False" />

    <!--平台不能输入的字符-->
    <add key="CharFilter" value="" />

    <!--是否连接Oracle数据库-->
    <add key="IsOracleDb" value="False" />

    <add key="TempFolder" value="D:\TempFolder"/>

  </appSettings>

